extends ../../layout

block content
  div(class='col-md-6')
    form(id='SpaceStation-delete-form' name='SpaceStation-delete-form' class='form-horizontal' method="POST" action='')
      input(id='id' type="hidden" name='id' value=SpaceStation.id class='form-control')
      div(class='form-group')
        div(class='col-md-12') Name: #{SpaceStation.name}; Capacity:  #{SpaceStation.capacity};
      div(class='form-group')
        div(class='col-md-12')
          button(type='submit' class='btn btn-primary') Delete
